{
  "__resolveType": "site/sections/Theme/Theme.tsx",
  "colorScheme": "any",
  "buttonStyle": {
    "--border-btn": "1px",
    "--rounded-btn": "0.2rem",
    "--btn-focus-scale": "0.9",
    "--animation-btn": "0.25s"
  },
  "otherStyles": {
    "--rounded-box": "1rem",
    "--rounded-badge": "1.9rem",
    "--animation-input": "0.2s",
    "--tab-border": "1px",
    "--tab-radius": "0.5rem"
  },
  "mode": "light",
  "mainColors": {
    "base-100": "#727efc",
    "primary": "#fdb800",
    "secondary": "#e48bae",
    "tertiary": "#ffffff",
    "neutral": "#000000",
    "info": "#6264d3",
    "error": "#ed5a5a",
    "warning": "#1e0c31",
    "success": "#53ee8f"
  },
  "font": {
    "__resolveType": "website/loaders/fonts/googleFonts.ts",
    "fonts": [
      {
        "variations": [
          {
            "weight": "400"
          }
        ],
        "family": "Playwrite Cuba"
      }
    ]
  }
}